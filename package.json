{
	"name": "skc-rcl",
	"version": "1.2.2",
	"description": "Component library for SKC Web and all other SKC Web sub domains.",
	"main": "dist/index.js",
	"module": "dist/index.es.js",
	"homepage": "http://ygo-skc.github.io/skc-rcl",
	"scripts": {
		"fresh-install": "rm -rf node_modules yarn.lock && yarn install",
		"start": "storybook dev -p 6006",
		"build": "rm -rf dist && rollup -c --configPlugin @rollup/plugin-typescript",
		"test": "jest",
		"test:i": "jest --watch",
		"build-storybook": "storybook build",
		"lint": "eslint src --ext .tsx",
		"deploy-storybook": "yarn run build-storybook && gh-pages -d storybook-static",
		"gh:deploy-storybook": "git remote set-url origin https://git:${GITHUB_TOKEN}@github.com/ygo-skc/skc-rcl.git && gh-pages -d storybook-static -u \"github-actions-bot <support+actions@github.com>\"",
		"ci:publish": "yarn run build && yarn publish"
	},
	"keywords": [
		"skc",
		"skc-site"
	],
	"author": "Javi Gomez",
	"license": "MIT",
	"resolutions": {
		"trim-newlines": "5.0.0",
		"glob-parent": "6.0.2",
		"trim": "1.0.1",
		"react-inspector": "6.0.2",
		"react-element-to-jsx-string": "17.0.1",
		"@mdx-js/react": "3.1.0",
		"d3-color": "3.1.0",
		"@babel/traverse": "7.27.4",
		"@babel/core": "7.27.4",
		"postcss": "8.5.4",
		"react-dom": "19.1.0",
		"ws": "8.18.2",
		"body-parser": "2.2.0",
		"path-to-regexp": "8.2.0",
		"send": "1.2.0",
		"serve-static": "2.2.0",
		"express": "5.1.0",
		"cross-spawn": "7.0.6",
		"rollup": "4.41.1"
	},
	"devDependencies": {
		"@babel/preset-env": "7.27.2",
		"@babel/preset-react": "7.27.1",
		"@babel/preset-typescript": "7.27.1",
		"@emotion/react": "11.14.0",
		"@emotion/styled": "11.14.0",
		"@mui/material": "7.1.1",
		"@nivo/pie": "0.99.0",
		"@rollup/plugin-babel": "6.0.4",
		"@rollup/plugin-image": "3.0.3",
		"@rollup/plugin-node-resolve": "16.0.1",
		"@rollup/plugin-terser": "0.4.4",
		"@rollup/plugin-typescript": "12.1.2",
		"@storybook/addon-essentials": "8.6.14",
		"@storybook/addon-interactions": "8.6.14",
		"@storybook/addon-links": "8.6.14",
		"@storybook/addon-webpack5-compiler-babel": "3.0.6",
		"@storybook/react-webpack5": "8.6.14",
		"@testing-library/jest-dom": "6.6.3",
		"@testing-library/react": "16.3.0",
		"@types/jest": "29.5.14",
		"@types/react": "19.1.6",
		"@types/rollup-plugin-peer-deps-external": "2.2.5",
		"@typescript-eslint/eslint-plugin": "8.33.0",
		"@typescript-eslint/parser": "8.33.0",
		"eslint": "8.57.1",
		"eslint-plugin-react": "7.37.5",
		"gh-pages": "6.3.0",
		"jest": "29.7.0",
		"jest-environment-jsdom": "29.7.0",
		"jest-transform-css": "6.0.3",
		"react": "19.1.0",
		"rollup": "4.41.1",
		"rollup-plugin-dts": "6.2.1",
		"rollup-plugin-peer-deps-external": "2.2.4",
		"rollup-plugin-postcss": "4.0.2",
		"storybook": "8.6.14",
		"ts-jest": "29.3.4",
		"typescript": "5.8.3"
	},
	"peerDependencies": {
		"@emotion/styled": ">=11.11.0",
		"@mui/material": ">=6.0.0",
		"@nivo/pie": ">=0.83.0",
		"react": ">=19.0.0"
	},
	"files": [
		"dist"
	],
	"jest": {
		"setupFilesAfterEnv": [
			"<rootDir>/src/setupTests.ts"
		],
		"testEnvironment": "jsdom",
		"collectCoverage": true,
		"collectCoverageFrom": [
			"./src/**"
		],
		"coverageThreshold": {
			"global": {
				"branches": 10,
				"functions": 20,
				"lines": 20,
				"statements": 20
			}
		},
		"transformIgnorePatterns": [],
		"coveragePathIgnorePatterns": [
			"src/css/",
			"src/img/",
			"src/stories/",
			"src/index.ts",
			"src/global.d.ts"
		],
		"transform": {
			"^.+\\.tsx?$": "ts-jest",
			".+\\.(css|styl|less|sass|scss)$": "jest-transform-css"
		}
	}
}
