{
	"name": "skc-rcl",
	"version": "1.1.5",
	"description": "Component library for SKC Web and all other SKC Web sub domains.",
	"main": "dist/index.js",
	"module": "dist/index.es.js",
	"homepage": "http://ygo-skc.github.io/skc-rcl",
	"scripts": {
		"preinstall": "npx force-resolutions",
		"postinstall": "npm dedupe",
		"fresh-install": "rm -rf node_modules package-lock.json && npm install",
		"start": "storybook dev -p 6006",
		"build": "rm -rf dist && rollup -c --configPlugin @rollup/plugin-typescript",
		"test": "jest",
		"test:i": "jest --watch",
		"build-storybook": "storybook build",
		"lint": "eslint src --ext .tsx",
		"deploy-storybook": "npm run build-storybook && gh-pages -d storybook-static",
		"gh:deploy-storybook": "git remote set-url origin https://git:${GITHUB_TOKEN}@github.com/ygo-skc/skc-rcl.git && gh-pages -d storybook-static -u \"github-actions-bot <support+actions@github.com>\"",
		"ci:publish": "npm run build && npm publish"
	},
	"keywords": [
		"skc",
		"skc-site"
	],
	"author": "Javi Gomez",
	"license": "MIT",
	"resolutions": {
		"trim-newlines": "5.0.0",
		"glob-parent": "6.0.2",
		"trim": "1.0.1",
		"react-inspector": "6.0.2",
		"react-element-to-jsx-string": "15.0.0",
		"@mdx-js/react": "3.0.1",
		"d3-color": "3.1.0",
		"@babel/traverse": "7.24.5"
	},
	"devDependencies": {
		"@babel/preset-env": "7.24.5",
		"@babel/preset-react": "7.24.1",
		"@babel/preset-typescript": "7.24.1",
		"@emotion/styled": "11.11.5",
		"@mui/material": "5.15.16",
		"@nivo/pie": "0.86.0",
		"@rollup/plugin-babel": "6.0.4",
		"@rollup/plugin-image": "3.0.3",
		"@rollup/plugin-node-resolve": "15.2.3",
		"@rollup/plugin-terser": "0.4.4",
		"@rollup/plugin-typescript": "11.1.6",
		"@storybook/addon-essentials": "8.0.9",
		"@storybook/addon-interactions": "8.0.9",
		"@storybook/addon-links": "8.0.9",
		"@storybook/addon-webpack5-compiler-babel": "3.0.3",
		"@storybook/react-webpack5": "8.0.9",
		"@testing-library/jest-dom": "6.4.2",
		"@testing-library/react": "15.0.6",
		"@types/jest": "29.5.12",
		"@types/react": "18.3.1",
		"@types/rollup-plugin-peer-deps-external": "2.2.5",
		"@typescript-eslint/eslint-plugin": "7.8.0",
		"@typescript-eslint/parser": "7.8.0",
		"eslint": "8.57.0",
		"eslint-plugin-react": "7.34.1",
		"gh-pages": "6.1.1",
		"jest": "29.7.0",
		"jest-environment-jsdom": "29.7.0",
		"jest-transform-css": "6.0.1",
		"react": "18.3.1",
		"rollup": "4.17.2",
		"rollup-plugin-dts": "6.1.0",
		"rollup-plugin-peer-deps-external": "2.2.4",
		"rollup-plugin-postcss": "4.0.2",
		"storybook": "8.0.9",
		"ts-jest": "29.1.2",
		"typescript": "5.4.5"
	},
	"peerDependencies": {
		"@emotion/styled": ">=11.11.0",
		"@mui/material": ">=5.14.6",
		"@nivo/pie": ">=0.83.0",
		"react": ">=18.3.1"
	},
	"files": [
		"dist"
	],
	"jest": {
		"setupFilesAfterEnv": [
			"<rootDir>/src/setupTests.ts"
		],
		"testEnvironment": "jsdom",
		"collectCoverage": true,
		"collectCoverageFrom": [
			"./src/**"
		],
		"coverageThreshold": {
			"global": {
				"branches": 12,
				"functions": 20,
				"lines": 20,
				"statements": 20
			}
		},
		"transformIgnorePatterns": [],
		"coveragePathIgnorePatterns": [
			"src/css/",
			"src/img/",
			"src/stories/",
			"src/index.ts",
			"src/global.d.ts"
		],
		"transform": {
			"^.+\\.tsx?$": "ts-jest",
			".+\\.(css|styl|less|sass|scss)$": "jest-transform-css"
		}
	}
}
